<%= @user.user_name %>

<%= form_with(model:@user,local: true) do |f| %>

<%= f.label :名前 %><br>
<%= f.text_field :name %><br>

<%= f.label :ユーザーネーム %><br>
<%= f.text_field :user_name %><br>

<%= f.label :メールアドレス %><br>
<%= f.email_field :email %><br>

<%= f.label :自己紹介 %><br>
<%= f.text_field :content %><br>

<%= f.submit "編集" %>


